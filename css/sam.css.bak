a.blue:link{color: #0000ff;text-decoration:underline;}
a.blue:active{color: #0000ff;text-decoration: underline;}
a.blue:visited{color: #0000ff;text-decoration:underline;}
a.blue:hover{color: #0000ff;text-decoration: underline;}

a.black:link{color: #000000;text-decoration:underline;}
a.black:active{color: #000000;text-decoration: underline;}
a.black:visited{color: #000000;text-decoration:underline;}
a.black:hover{color: #000000;text-decoration: underline;}

a.red:link{color: #ff0000;text-decoration:underline;}
a.red:active{color: #ff0000;text-decoration: underline;}
a.red:visited{color: #ff0000;text-decoration:underline;}
a.red:hover{color: #ff0000;text-decoration: underline;}

a.boldbluelink:link{color: #0000ff;text-decoration:underline;font-weight:bolder}
a.boldbluelink:active{color: #0000ff;text-decoration: underline;font-weight:bolder}
a.boldbluelink:visited{color: #0000ff;text-decoration:underline;font-weight:bolder}
a.boldbluelink:hover{color: #0000ff;text-decoration: underline;font-weight:bolder}

a.smallbluelink:link{color: #0000ff;text-decoration:underline;font-size:10px}
a.smallbluelink:active{color: #0000ff;text-decoration: underline;font-size:10px}
a.smallbluelink:visited{color: #0000ff;text-decoration:underline;font-size:10px}
a.smallbluelink:hover{color: #0000ff;text-decoration: underline;font-size:10px}

